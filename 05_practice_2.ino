void setup() {
  // put your setup code here, to run once:
pinMode(7, OUTPUT); //디지털 7번핀 출력모드로 설정.
}

void loop() {
  // put your main code here, to run repeatedly
  digitalWrite(7, LOW);
  delay(1000);
  digitalWrite(7, HIGH);
  delay(100);
  digitalWrite(7, LOW);
  delay(100);
  digitalWrite(7, HIGH);
  delay(100);
  digitalWrite(7, LOW);
  delay(100);
  digitalWrite(7, HIGH);
  delay(100);
  digitalWrite(7, LOW);
  delay(100);
  digitalWrite(7, HIGH);
  delay(100);
  digitalWrite(7, LOW);
  delay(100);
  digitalWrite(7, HIGH);
  delay(100);
  digitalWrite(7, LOW);
  delay(100);
  digitalWrite(7, HIGH);
  while(1) {}
  


}
